#include <iostream>
#include <string>
#include <cctype>

using namespace std;

bool checkFirstPart(string firstPart) {

    if (firstPart.length() < 1 || firstPart.length() > 64) return false;
    if (firstPart[0] == '.' || firstPart.back() == '.') return false;
    for (size_t i = 0; i < firstPart.length() - 1; i++) {
        if (firstPart[i] == '.' && firstPart[i + 1] == '.') return false;
    }
    for (char c : firstPart) {
        if (isalnum(c)) continue; 
        if (c == '.' || c == '!' || c == '#' || c == '$' || c == '%' || c == '&' || c == '\'' ||
            c == '*' || c == '+' || c == '/' || c == '=' || c == '?' || c == '^' ||
            c == '`' || c == '{' || c == '|' || c == '}' || c == '-' || c == '_' || c == '~') continue;
        return false;
    }
    return true;
}

bool checkSecondPart(string secondPart) {
    if (secondPart.length() < 1 || secondPart.length() > 63) return false;
    if (secondPart[0] == '.' || secondPart.back() == '.') return false;
    for (size_t i = 0; i < secondPart.length() - 1; i++) {
        if (secondPart[i] == '.' && secondPart[i + 1] == '.') return false;
    }
    for (char c : secondPart) {
        if (isalnum(c)) continue; 
        if (c == '.' || c == '-') continue;  
        return false;
    }
    return true;
}

bool isValidEmail(string email) {
    size_t atPos = email.find('@');
    if (atPos == string::npos || email.find('@', atPos + 1) != string::npos) {
        return false;
    }

    string firstPart = email.substr(0, atPos);
    string secondPart = email.substr(atPos + 1);

    return checkFirstPart(firstPart) && checkSecondPart(secondPart);
}

int main() {
    setlocale(LC_ALL, "RU");
    string email;
    cout << "Введите email: "; getline(cin, email);

    if (isValidEmail(email)) {
        cout << "Yes" << endl;
    }
    else {
        cout << "No" << endl;
    }
    return 0;
}
